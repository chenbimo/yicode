{
    "name": "@yicode/yite-cli",
    "version": "2.6.33",
    "description": "专注于vite + vue3 的项目开发脚手架",
    "main": "./index.js",
    "exports": {
        "./*": "./*"
    },
    "bin": {
        "yite": "bin.js"
    },
    "type": "module",
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "scripts": {
        "r": "release-it"
    },
    "release-it": {
        "git": false,
        "plugins": {
            "@release-it/bumper": {
                "out": {
                    "file": "package.json",
                    "path": [
                        "dependencies.@yicode/yidash"
                    ]
                }
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/chenbimo/yite-cli"
    },
    "homepage": "https://chensuiyi.me",
    "keywords": [
        "vite",
        "vue.js",
        "vue-cli",
        "webpack",
        "cli",
        "vue",
        "web",
        "脚手架"
    ],
    "author": "chensuiyi <bimostyle@qq.com>",
    "dependencies": {
        "@unocss/transformer-compile-class": "^0.61.5",
        "@unocss/transformer-directives": "^0.61.5",
        "@unocss/transformer-variant-group": "^0.61.5",
        "@vitejs/plugin-vue": "^5.1.0",
        "@vue-macros/reactivity-transform": "^1.0.0",
        "colorette": "^2.0.20",
        "copy-anything": "^4.0.2",
        "fast-glob": "^3.3.2",
        "fs-extra": "^11.2.0",
        "got": "^14.4.2",
        "lodash-es": "^4.17.21",
        "log-symbols": "^6.0.0",
        "merge-anything": "^6.0.2",
        "pacote": "^18.0.6",
        "portfinder": "^1.0.32",
        "rollup-plugin-visualizer": "^5.12.0",
        "tiny-cookie": "^2.5.1",
        "unocss": "^0.61.5",
        "unplugin-auto-import": "^0.18.2",
        "unplugin-icons": "^0.19.0",
        "unplugin-vue-components": "^0.27.3",
        "vite": "^5.3.5",
        "vite-plugin-vue-devtools": "^7.3.7",
        "vue-i18n": "^9.13.1"
    },
    "gitHead": "130b0428722cebb2a4170fea95c08a398b50eb23"
}
